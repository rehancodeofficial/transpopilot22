# TranspoPilot AI Environment Variables

# API Configuration
VITE_API_URL=https://api.transpopilot.ai
VITE_ENVIRONMENT=production

# Supabase Configuration (if using)
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# API Monitoring (optional)
# Set to 'true' to completely disable API performance monitoring and logging
# This can be useful for troubleshooting or if monitoring causes issues
VITE_DISABLE_API_MONITORING=false

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Contact Form (optional)
VITE_CONTACT_FORM_ENDPOINT=https://formspree.io/f/your-form-id